<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="onlineclass.exam.mapper.ExamMapper">
    <select id="getLastExam" resultType="onlineclass.exam.model.Exam">
        SELECT * FROM exams WHERE eid
        in (select max(eid) from exams);
    </select>
</mapper>